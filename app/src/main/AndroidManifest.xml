<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.Yuni.MuslimApp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="Muslim App"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.Yuni.MuslimApp.MainActivity"/>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.Yuni.MuslimApp.Activity_Jadwal"/>
        <activity android:name="com.Yuni.MuslimApp.Activity_Halal"/>
        <activity android:theme="@style/AppTheme" android:name=".ActivityAsmaulHusna"/>
        <activity android:theme="@style/AppTheme.NoActionBar" android:name=".ActivityAsmaulHusnaDetail"/>
        <activity android:name=".Activity_DoaHarian"/>
        <activity android:name=".PagiActivity"/>
        <activity android:name=".SoreActivity"/>
    </application>

</manifest>